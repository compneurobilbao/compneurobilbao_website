	number = {9},
	journaltitle = {Movement Disorders},
	author = {Murueta‐Goyena, Ane and Del Pino, Rocío and Reyero, Paula and Galdós, Marta and Arana, Begoña and Lucas‐Jiménez, Olaia and Acera, Marian and Tijero, Beatriz and Ibarretxe‐Bilbao, Naroa and Ojeda, Natalia and Peña, Javier and Cortés, Jesús and Gómez‐Esteban, Juan Carlos and Gabilondo, Iñigo},
	urldate = {2024-04-15},
	date = {2019-09},
