	keywords = {Humans, Male, Brain, Aged, Female, Image Processing, Computer-Assisted, magnetic resonance imaging, Neuropsychological Tests, Cognition, Statistics as Topic, Models, Neurological, Cohort Studies, Mental Status Schedule, Sex Factors, Age Factors, age-related cognitive decline, brain volume, Cognitive Aging, Longitudinal study, structural {MRI}, white matter hyperintensities},
