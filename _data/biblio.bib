@article{cortes_detection_2017,
	title = {Detection of Intratumor Heterogeneity in Modern Pathology: A Multisite Tumor Sampling Perspective},
	volume = {4},
	doi = {10.3389/fmed.2017.00025},
	shorttitle = {Detection of Intratumor Heterogeneity in Modern Pathology},
	abstract = {Current sampling protocols of neoplasms along the digestive tract and in the urinary bladder have to be updated, as they do not respond to the necessities of modern personalized medicine. We show here that an adapted version of multisite tumor sampling ({MSTS}) is a sustainable model to overcome current deficiencies in digestive and bladder tumors when they are large enough so as to make unaffordable a total sampling. The new method is based on the divide-and-conquer algorithm and includes a slight modification of the {MSTS}, which proved to be useful very recently in clear cell renal cell carcinoma. This in silico analysis confirms the usefulness of {MSTS} for detecting intratumor heterogeneity ({ITH}) in tumors arising in hollow viscera. However, {MSTS} does not seem to improve routine traditional sampling in detecting tumor budding, extramural venous invasion, and perineural invasion. We conclude that (1) {MSTS} is the best method for tumor sampling to detect {ITH} balancing high performance and sustainable cost, (2) {MSTS} must be adapted to tumor shape and tumor location for an optimal performance.},
	pages = {25},
	journaltitle = {Front Med (Lausanne)},
	author = {Cortés, Jesús M. and de Petris, Giovanni and López, José I.},
	date = {2017},
	pmid = {28321395},
	pmcid = {PMC5337957},
	keywords = {in silico modeling, intratumor heterogeneity, large bowel, stomach, tumor sampling, urinary bladder},
}

