@article{marro_chaotic_2007,
	title = {Chaotic hopping between attractors in neural networks},
	volume = {20},
	issn = {0893-6080},
	doi = {10.1016/j.neunet.2006.11.005},
	abstract = {We present a neurobiologically-inspired stochastic cellular automaton whose state jumps with time between the attractors corresponding to a series of stored patterns. The jumping varies from regular to chaotic as the model parameters are modified. The resulting irregular behavior, which mimics the state of attention in which a system shows a great adaptability to changing stimulus, is a consequence in the model of short-time presynaptic noise which induces synaptic depression. We discuss results from both a mean-field analysis and Monte Carlo simulations.},
	pages = {230--235},
	number = {2},
	journaltitle = {Neural Netw},
	author = {Marro, Joaquín and Torres, Joaquín J. and Cortés, Jesús M.},
	date = {2007-03},
