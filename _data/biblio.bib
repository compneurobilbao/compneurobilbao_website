	doi = {10.1101/2020.07.17.208132},
	file = {Full Text:files/544/Jimenez-Marin et al. - 2020 - Transcriptional Signatures of Synaptic Vesicle Gen.pdf:application/pdf},
}

@article{bueicheku_central_2020-1,
