	pmid = {15596617},
	keywords = {Humans, Male, Aged, Female, Middle Aged, Cognition Disorders, Neuropsychological Tests, Myotonic Dystrophy, Chi-Square Distribution, Psychiatric Status Rating Scales},
	file = {Full Text:files/598/Modoni et al. - 2004 - Characterization of the pattern of cognitive impai.pdf:application/pdf},
}

