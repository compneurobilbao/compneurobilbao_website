	date = {2011-10-18},
	pmid = {21975202},
	pmcid = {PMC3198977},
	keywords = {Humans, Male, Brain, Nerve Net, Aged, Aging, Female, Middle Aged, Brain Mapping, Image Processing, Computer-Assisted, magnetic resonance imaging, Neuropsychological Tests, Statistics, Nonparametric, Aged, 80 and over, Neural Pathways, Alzheimer Disease, Oxygen, Cohort Studies, Psychiatric Status Rating Scales, Age Factors},
