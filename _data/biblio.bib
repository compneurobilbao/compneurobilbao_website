@article{osullivan_evidence_2001,
	title = {Evidence for cortical "disconnection" as a mechanism of age-related cognitive decline},
	volume = {57},
	issn = {0028-3878},
	abstract = {{BACKGROUND}: Normal aging is accompanied by a decline of cognitive abilities, and executive skills may be affected selectively, but the underlying mechanisms remain obscure and preventive strategies are lacking. It has been suggested that cortical "disconnection" due to the loss of white matter fibers may play an important role. But, to date, there has been no direct demonstration of structural disconnection in humans in vivo.
{METHODS}: The authors used diffusion tensor {MRI} to look for evidence of ultrastructural changes in cerebral white matter in a group of 20 elderly volunteers with normal conventional {MRI} scans, and a group of 10 younger controls. The older group also underwent neuropsychological assessment.
{RESULTS}: Diffusional anisotropy, a marker of white matter tract integrity, was reduced in the white matter of older subjects and fell linearly with increasing age in the older group. Mean diffusivity was higher in the older group and increased with age. These changes were maximal in anterior white matter. In the older group, anterior mean diffusivity correlated with executive function assessed by the Trail Making Test.
{CONCLUSIONS}: These findings provide direct evidence that white matter tract disruption occurs in normal aging and would be consistent with the cortical disconnection hypothesis of age-related cognitive decline. Maximal changes in anterior white matter provide a plausible structural basis for selective loss of executive functions. In addition to providing new information about the biological basis of cognitive abilities, diffusion tensor {MRI} may be a sensitive tool for assessing interventions aimed at preventing cognitive decline.},
	pages = {632--638},
	number = {4},
	journaltitle = {Neurology},
	author = {O'Sullivan, M. and Jones, D. K. and Summers, P. E. and Morris, R. G. and Williams, S. C. and Markus, H. S.},
	date = {2001-08-28},
	pmid = {11524471},
	keywords = {Humans, Male, Adult, Aged, Aging, Female, Middle Aged, Nerve Fibers, Myelinated, magnetic resonance imaging, cerebral cortex, Corpus Callosum, Memory Disorders, Trail Making Test},
}

